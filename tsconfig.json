{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "lib": ["DOM", "ES5", "ES6", "ES2015.Promise"],
    "sourceMap": true,
    "noImplicitAny": true,
    "importHelpers": false,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "fullstory-form-core": ["./src/entries/core.ts"],
      "@coreIconsV1": ["packages/form-core/src/iconsV1"],
      "@coreIconsV2": ["packages/form-core/src/iconsV2"]
    }
  },
  "exclude": ["src/entries/**/*.ts"],
  "include": [
    "src/**/*.ts",
    "packages/form-core/entries/chunks/core-wo-model.ts"
  ]
}
